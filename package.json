{
	"name": "ts-codegen",
	"version": "0.1.0",
	"description": "TypeScript boilerplate code generator for NodeJS Projects in general.",
	"main": "src/index.ts",
	"scripts": {
		"start": "node dist/index.js",
		"start2": "ts-node src/index.ts",
		"dev": "nodemon dist/index.js",
		"dev2": "nodemon --exec ts-node src/index.ts",
		"build": "tsc",
		"watch": "tsc -w",
		"build-script": "tsc && shx rm -rf dist/templates && shx cp -r src/templates dist"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jethrinfox/ts-codegen.git"
	},
	"keywords": [
		"ts",
		"typescript",
		"boilerplate",
		"code",
		"gen",
		"generator"
	],
	"author": "jethrinfox",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/jethrinfox/ts-codegen/issues"
	},
	"homepage": "https://github.com/jethrinfox/ts-codegen#readme",
	"devDependencies": {
		"@types/ejs": "^3.0.5",
		"@types/inquirer": "^7.3.1",
		"@types/node": "^14.14.20",
		"nodemon": "^2.0.7",
		"shx": "^0.3.3",
		"ts-node": "^9.1.1",
		"typescript": "^4.1.3"
	},
	"dependencies": {
		"ejs": "^3.1.5",
		"inquirer": "^7.3.3"
	},
	"bin": {
		"template-generator": "./dist/index.js"
	}
}
